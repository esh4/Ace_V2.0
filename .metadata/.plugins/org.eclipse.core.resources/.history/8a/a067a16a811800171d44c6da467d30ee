//import edu.wpi.first.wpilibj.PIDOutput;
import edu.wpi.first.wpilibj.PIDSource;


public class DummyPIDSource implements PIDSource{
    double source;
    
    public DummyPIDSource()
    {
        source = 0;
    }

    public void setValue(double value) {
        this.source = value;
    }

    public double pidGet() {
        return source;
    }
    
}
